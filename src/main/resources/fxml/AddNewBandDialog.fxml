<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="anchorPane" prefHeight="540.0" prefWidth="540.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="semestralka.controllers.AddNewBandDialogController">
    <stylesheets>
        <URL value="@/styles/mainscreen.css" />
    </stylesheets>
    <children>
      <Label layoutX="176.0" layoutY="190.0" prefHeight="43.0" prefWidth="188.0" text="Add new band" textFill="#003663">
         <font>
            <Font name="AvenirNextLTPro-MediumCn" size="35.0" />
         </font>
      </Label>
      <TextField fx:id="input_name" layoutX="120.0" layoutY="241.0" prefHeight="60.0" prefWidth="301.0" promptText="Enter band's name" styleClass="input">
         <font>
            <Font name="AvenirNextLTPro-MediumCn" size="22.0" />
         </font>
      </TextField>
      <Button fx:id="submit" layoutX="196.0" layoutY="312.0" mnemonicParsing="false" onAction="#submit" text="Save to database">
         <font>
            <Font name="AvenirNextLTPro-MediumCn" size="20.0" />
         </font>
      </Button>
      <Button fx:id="close" layoutX="472.0" layoutY="14.0" mnemonicParsing="false" onAction="#close" styleClass="close" text="Close">
         <font>
            <Font name="AvenirNextLTPro-MediumCn" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
